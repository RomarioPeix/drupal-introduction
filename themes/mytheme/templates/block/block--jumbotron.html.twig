{% block content %}
  <section class="jumbotron text-center">
    <div class="container">
      <h1 class="jumbotron-heading">{{ content.field_title.0 }}</h1>
      <p class="lead text-muted">{{ content.field_description.0}}</p>
      <p>
        <a href="{{ content.field_primary_cta.0['#url'] }}" class="btn btn-primary my-2">
          {{ content.field_primary_cta.0['#title'] }}
        </a>
        <a href="{{ content.field_secondary_cta.0['#url'] }}" class="btn btn-secondary my-2">
          {{ content.field_secondary_cta.0['#title'] }}
        </a>
      </p>
    </div>
  </section>
{% endblock %}
